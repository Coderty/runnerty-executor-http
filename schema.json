{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "@runnerty-executor-http",
  "definitions": {
    "config": {
      "type": "object",
      "required": ["id", "type"],
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "pattern": "@runnerty-executor-http"
        }
      }
    },
    "params": {
      "type": "object",
      "required": ["id", "method", "url"],
      "properties": {
        "id": {
          "type": "string"
        },
        "method": {
          "type": "string",
          "enum": ["get", "post", "delete", "put", "patch", "head", "options", "connect", "trace"]
        },
        "url": {
          "type": "string"
        },
        "headers": {
          "type": "object"
        },
        "params": {
          "type": "object"
        },
        "paramsSerializerOptions": {
          "type": "object",
          "items": {
            "indices": {
              "type": "boolean"
            },
            "allowDots": {
              "type": "boolean"
            },
            "addQueryPrefix": {
              "type": "boolean"
            },
            "encode": {
              "type": "boolean"
            },
            "encodeValuesOnly": {
              "type": "boolean"
            },
            "strictNullHandling": {
              "type": "boolean"
            },
            "skipNulls": {
              "type": "boolean"
            },
            "charset": {
              "type": "string"
            },
            "charsetSentinel": {
              "type": "boolean"
            },
            "delimiter": {
              "type": "string"
            },
            "arrayFormat": {
              "type": "string",
              "enum": ["indices", "brackets", "repeat", "comma", null]
            }
          }
        },
        "data": {
          "type": ["string", "object"]
        },
        "timeout": {
          "type": "number"
        },
        "withCredentials": {
          "type": "boolean"
        },
        "auth": {
          "type": "object",
          "items": {
            "username": {
              "type": "string"
            },
            "password": {
              "type": "string"
            }
          }
        },
        "responseType": {
          "type": "string",
          "default": "json",
          "enum": ["json", "arraybuffer", "document", "text", "stream", "blob"]
        },
        "responseEncoding": {
          "type": "string",
          "default": "utf8"
        },
        "xsrfCookieName": {
          "type": "string",
          "default": "XSRF-TOKEN"
        },
        "xsrfHeaderName": {
          "type": "string",
          "default": "X-XSRF-TOKEN"
        },
        "maxContentLength": {
          "type": "number"
        },
        "maxBodyLength": {
          "type": "number"
        },
        "maxRedirects": {
          "type": "number",
          "default": 5
        },
        "socketPath": {
          "type": "string"
        },
        "httpAgent": {
          "type": "object",
          "items": {
            "keepAlive": {
              "type": "boolean"
            },
            "keepAliveMsecs": {
              "type": "number"
            },
            "maxSockets": {
              "type": "number"
            },
            "maxFreeSockets": {
              "type": "number"
            },
            "timeout": {
              "type": "number"
            }
          }
        },
        "httpsAgent": {
          "type": "object",
          "items": {
            "keepAlive": {
              "type": "boolean"
            },
            "maxCachedSessions": {
              "type": "number"
            },
            "servername": {
              "type": "string"
            },
            "passphrase": {
              "type": "string"
            },
            "ca": {
              "type": "string"
            },
            "cert": {
              "type": "string"
            },
            "key": {
              "type": "string"
            },
            "pfx": {
              "type": "string"
            },
            "ca_file": {
              "type": "string"
            },
            "cert_file": {
              "type": "string"
            },
            "key_file": {
              "type": "string"
            },
            "pfx_file": {
              "type": "string"
            }
          }
        },
        "proxy": {
          "type": "object",
          "items": {
            "host": {
              "type": "string"
            },
            "port": {
              "type": "number"
            },
            "auth": {
              "type": "object",
              "items": {
                "username": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        },
        "decompress": {
          "type": "boolean"
        },
        "files": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["name", "path"],
            "properties": {
              "name": {
                "type": "string"
              },
              "path": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
